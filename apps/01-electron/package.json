{
  "name": "desktop-shootout-electron",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p electron/tsconfig.json",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron:wait\"",
    "electron:wait": "wait-on tcp:5173 && tsc -p electron/tsconfig.json && electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@mantine/core": "^7.16.2",
    "@mantine/hooks": "^7.16.2",
    "@tabler/icons-react": "^3.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.1",
    "systeminformation": "^5.25.11"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.1.0",
    "electron-builder": "^25.1.8",
    "postcss": "^8.5.1",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "wait-on": "^8.0.2"
  }
}
